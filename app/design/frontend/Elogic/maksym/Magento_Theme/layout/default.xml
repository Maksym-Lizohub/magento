<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <!--header-->
        <referenceContainer name="header.panel.wrapper" remove="true"></referenceContainer>

        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/logo.png</argument>
                <argument name="logo_width" xsi:type="number">176</argument>
                <argument name="logo_height" xsi:type="number">46</argument>
                <argument name="logo_alt" xsi:type="string" translate="true">Logo</argument>
            </arguments>
        </referenceBlock>

        <referenceContainer name="title" remove="true"></referenceContainer>
        <!-- Shop By Brands -->

       

    </body>
</page>
